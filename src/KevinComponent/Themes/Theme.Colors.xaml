<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!--  FlexGrid  -->
	<SolidColorBrush x:Key="FlexGrid.Static.Background" Color="{x:Static SystemColors.ControlColor}" />
	<SolidColorBrush x:Key="FlexGrid.Static.Foreground" Color="#222222" />
	<SolidColorBrush x:Key="FlexGrid.Static.Border" Color="#D5DFE8" />
	<SolidColorBrush x:Key="BandHeader.Static.Background" Color="{x:Static SystemColors.ControlLightColor}" />
	<SolidColorBrush x:Key="BandHeader.Static.Border" Color="Black" />
	<SolidColorBrush x:Key="BandHeader.MouseOver.Background" Color="{x:Static SystemColors.ControlColor}" />
	<SolidColorBrush x:Key="BandHeader.MouseOver.Border" Color="Black" />
	<SolidColorBrush x:Key="BandHeader.Pressed.Background" Color="{x:Static SystemColors.ControlDarkColor}" />
	<SolidColorBrush x:Key="BandHeader.Pressed.Border" Color="Black" />
	<SolidColorBrush x:Key="BandHeader.Disabled.Background" Color="#F4F4F4" />
	<SolidColorBrush x:Key="BandHeader.Disabled.Border" Color="Black" />
	<SolidColorBrush x:Key="BandHeader.Disabled.Foreground" Color="#222222" />

</ResourceDictionary>